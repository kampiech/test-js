{
  "name": "remix-example",
  "version": "5.8.58",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "remix build && run-p \"dev:*\"",
    "dev:node": "cross-env NODE_ENV=development nodemon ./server.js --watch ./server.js",
    "dev:remix": "remix watch",
    "start": "cross-env NODE_ENV=production node ./server.js"
  },
  "dependencies": {
    "@grupa-pracuj/juno-config": "^5.8.56",
    "@grupa-pracuj/juno-discovery": "^5.8.58",
    "@grupa-pracuj/juno-express": "^5.8.58",
    "@grupa-pracuj/juno-logger": "^5.8.57",
    "@remix-run/express": "1.6.7",
    "@remix-run/node": "1.6.7",
    "@remix-run/react": "1.6.7",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "express": "4.18.1",
    "morgan": "1.10.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@remix-run/dev": "1.6.7",
    "@remix-run/eslint-config": "1.6.7",
    "eslint": "8.20.0",
    "nodemon": "2.0.19",
    "npm-run-all": "4.1.5"
  },
  "engines": {
    "node": ">=18"
  }
}
